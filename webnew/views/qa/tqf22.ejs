<%- include ../header.ejs %>
	<body>
        
        <td>
            <div class="container-fluid">
            <div class="row-fluid">
            <div class="span15">
            <div class="col-md-15">
			
				<h3>Lectures and Instructors</h3>
                

				<h5>Table : List of lectures and instructors in charge /Academic Year: {{year}}</h5> 
    <% if(program != null){%>  
    	 <% var i,j,k;
			for(i=0;i< program.length;i++){%>	  
        
                <h5>Year:  <%= program[i].Year %> Semester:<%= program[i].semester %> </h5>
                <div id="tab_tqf2"><table class="table muffin">
                <thead align="center">
                    <tr>
                        <th width="40"><a href="javascript:table_sort('tab_2',0)">Code</a></th>
                        <th><a href="javascript:table_sort('tab_tqf2',1)">Subject</a></th>
                        <th><a href="javascript:table_sort('tab_tqf2',2)">Credits</a></th>
                        <th><a href="javascript:table_sort('tab_tqf2',3)">Lectures</a></th>
                        <th colspan=4><a href="javascript:table_sort('tab_tqf2',4)">Registered year of students</a></th>
                    </tr>
                    <tr>
                      <th colspan=1></th>
                      <th colspan=1></th>
                      <th colspan=1></th>
                      <th colspan=1></th>
                      <th>B.E. 2557</th>
                      <th>B.E. 2556</th>
                      <th>Before B.E. 2556</th>
                    </tr>
                </thead>
                <tbody>
                 <%   for(j=0;j< program[i].subject.length;j++){%>
                    <tr>
                    <th colspan=1><%= program[i].subject[j]._id %> </th>
                    <th colspan=1><%= program[i].subject[j].sub_name %> </th>
                    <th colspan=1><%= program[i].subject[j].sub_credit %></th>
                    <th colspan=1>
                   <%   for(k=0;k< program[i].subject[j].sub_lecter.length;k++){%>
                      <%= program[i].subject[j].sub_lecter[k].local.title %>
                      <%= program[i].subject[j].sub_lecter[k].local.name %> 
                      <%= program[i].subject[j].sub_lecter[k].local.surname %> <br>
                                                
                     <%}%>
                     </th>
                    <th> </th>
                    <th> </th>
                    </tr>
                    <tr>
                   <%}%>
                </tbody>
                </table>
              </div>

       <%}%>

        <br>
        <br>
        <br>
			  <h5>Table : Qualifications of instructors in CiES program, 2014</h5>

       
			  <div id="tab_tqf2"><table class="table muffin">
                <thead align="center">
                    <tr>
                        <th width="40"><a href="javascript:table_sort('tab_2',0)">No.</a></th>
                        <th><a href="javascript:table_sort('tab_tqf2',1)">Title</a></th>
                        <th><a href="javascript:table_sort('tab_tqf2',2)">Name-Surname</a></th>
                        <th colspan=2><a href="javascript:table_sort('tab_tqf2',3)">Education</a></th>
                        <th colspan=4><a href="javascript:table_sort('tab_tqf2',4)">Teaching Experiences</a></th>
                    </tr>
                    <tr>
                      <th colspan=1></th>
                      <th colspan=1></th>
                      <th colspan=1></th>
                      <th>Degree</th>
                      <th>University</th>
					   <th colspan=1></th>
                      
                    </tr>
                    </thead>
                    <tbody>
            <% var i,j,k,l;
			for(i=0;i< program.length;i++){%>
                
				 <%   for(j=0;j< program[i].subject.length;j++){%>
                  
                    <%   for(k=0;k< program[i].subject[j].sub_lecter.length;k++){%>
				          	<tr>
                      <th colspan=1><%= 1 %></th>
                      <th colspan=1><%= program[i].subject[j].sub_lecter[k].local.title %></th>
                      <th colspan=1><%= program[i].subject[j].sub_lecter[k].local.name %> 
                      				<%= program[i].subject[j].sub_lecter[k].local.surname %> </th>
                      
                      <th>
                        <%= program[i].subject[j].sub_lecter[k].education[0].degree %>
                      </th>
                      <th> <%= program[i].subject[j].sub_lecter[k].education[0].university %></th>
                     
                      <th>{{local.teachingExperience}}</th>                     
                    </tr>
                    <%}%>
                  <%}%>
              <%}%>  
                </tbody>
                </table>
              </div>

          </div>
        </td>
       <%}else{%>
			<strong>No program</strong> 
		<%} %>		



</body>
	
				
	

	
<%- include ../footer.ejs %>